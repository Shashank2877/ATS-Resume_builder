# Production Environment
VITE_APP_ENV=production
VITE_API_BASE_URL=https://your-production-api.com/api
VITE_AI_SERVICE_URL=https://your-ai-service.com/ai
VITE_APP_NAME=ATS Resume Builder
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# Authentication
VITE_JWT_SECRET=your_secure_production_jwt_secret
VITE_TOKEN_EXPIRE_TIME=24h

# AI Service Configuration
VITE_AI_MODEL=gpt-4
VITE_AI_MAX_TOKENS=2000
VITE_AI_TEMPERATURE=0.7

# Feature Flags
VITE_ENABLE_AI_GENERATION=true
VITE_ENABLE_PDF_EXPORT=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_LOGS=false

# Storage
VITE_LOCAL_STORAGE_PREFIX=ats_resume_
VITE_SESSION_STORAGE_PREFIX=ats_session_