# Staging Environment
VITE_APP_ENV=staging
VITE_API_BASE_URL=https://staging-api.your-domain.com/api
VITE_AI_SERVICE_URL=https://staging-ai.your-domain.com/ai
VITE_APP_NAME=ATS Resume Builder (Staging)
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# Authentication
VITE_JWT_SECRET=staging_jwt_secret_key
VITE_TOKEN_EXPIRE_TIME=24h

# AI Service Configuration
VITE_AI_MODEL=gpt-3.5-turbo
VITE_AI_MAX_TOKENS=1500
VITE_AI_TEMPERATURE=0.7

# Feature Flags
VITE_ENABLE_AI_GENERATION=true
VITE_ENABLE_PDF_EXPORT=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_LOGS=true

# Storage
VITE_LOCAL_STORAGE_PREFIX=ats_resume_staging_
VITE_SESSION_STORAGE_PREFIX=ats_session_staging_